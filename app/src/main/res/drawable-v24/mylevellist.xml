<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 值为0~5时显示的图片 -->
    <item
        android:minLevel="0"
        android:maxLevel="4"
        android:drawable="@drawable/wifi0" />

    <!-- 值为6~10时显示的图片 -->
